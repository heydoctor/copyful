{
  "name": "copyful",
  "version": "0.1.11",
  "description": "Copy management, simplified.",
  "main": "lib/copyful.js",
  "module": "lib/copyful.esm.js",
  "types": "lib/copyful.d.ts",
  "repository": "https://github.com/heydoctor/copyful",
  "author": "Gerrymi Bernardo <gerrymibernardo@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "bump": "standard-version",
    "format": "prettier --write '{src,example}/**/*.ts' '{src,example}/**/*.tsx'",
    "test": "jest --verbose",
    "build": "npm-run-all clean rollup",
    "clean": "rimraf lib",
    "rollup": "rollup -c",
    "release": "npm-run-all test build bump",
    "example": "parcel example/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": ">16.8.0",
    "react-dom": ">16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-typescript": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@types/jest": "^25.2.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "babel-plugin-typescript-to-proptypes": "^1.3.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "standard-version": "^8.0.0",
    "ts-jest": "^25.5.1",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
